{
    "name": "madeweb/eloquent-api",
    "description": "An Eloquent model with support for API, is a wrapper for use endpoints how to model entities",
    "type": "library",
    "license": "GPL-3.0-only",
    "keywords": [
        "api",
        "microservices",
        "eloquent"
    ],
    "authors": [
        {
            "name": "Luis Benavides",
            "email": "lbenavides@madewebsolutions.com"
        }
    ],
    "require": {
        "illuminate/database": "^5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Madeweb\\Eloquent\\API\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Madeweb\\Eloquent\\API\\EloquentAPIServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
